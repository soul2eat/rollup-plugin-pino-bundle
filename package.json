{
    "name": "rollup-plugin-pino-bundle",
    "version": "1.0.0",
    "description": "Rollup plugin for bundling PinoJS logger with transports",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "type": "module",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.mjs"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            },
            "default": "./dist/index.mjs"
        },
        "./dist/*": {
            "types": "./dist/*.d.ts",
            "import": "./dist/*.mjs",
            "require": "./dist/*.cjs"
        }
    },
    "scripts": {
        "build": "tsc && tsup"
    },
    "keywords": [
        "vite-plugin",
        "rollup-plugin",
        "pino",
        "bundle",
        "pino-rollup",
        "rollup-plugin-pino-bundle",
        "logger",
        "logging",
        "pino-transport"
    ],
    "author": {
        "name": "SoulEat",
        "email": "souleat.work@gmail.com",
        "url": "https://github.com/soul2eat"
    },
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^22.15.30",
        "rollup": "^4.40.2",
        "tsup": "^8.5.0",
        "typescript": "^5.8.3",
        "vite": "^6.3.5"
    },
    "dependencies": {
        "@rollup/pluginutils": "^5.1.4"
    }
}
